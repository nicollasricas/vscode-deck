{
  "name": "vscode-streamdeck",
  "displayName": "Stream Deck for Visual Studio Code",
  "description": "Enables Stream Deck integration with Visual Studio Code.",
  "version": "4.1.5",
  "engines": {
    "vscode": "^1.38.0"
  },
  "bugs": {
    "url": "https://github.com/nicollasricas/vscode-streamdeck/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nicollasricas/vscode-streamdeck.git"
  },
  "publisher": "nicollasr",
  "author": {
    "name": "Nicollas R."
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "keywords": [
    "streamdeck",
    "deck",
    "stream",
    "elgato"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "category": "Stream Deck",
        "command": "streamdeck.reconnectToServer",
        "title": "Reconnect"
      },
      {
        "category": "Stream Deck",
        "command": "streamdeck.activateSession",
        "title": "Activate"
      }
    ],
    "configuration": [
      {
        "title": "Stream Deck",
        "properties": {
          "streamdeck.serverHost": {
            "default": "127.0.0.1",
            "type": "string"
          },
          "streamdeck.serverPort": {
            "default": 48969,
            "type": "integer"
          }
        }
      }
    ]
  },
  "extensionKind": [
    "ui"
  ],
  "icon": "code.png",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "strongly-typed-events": "^1.6.11",
    "ws": "^7.4.6"
  },
  "devDependencies": {
    "@types/vscode": "^1.38.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.5",
    "@types/ws": "7.2.4",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
